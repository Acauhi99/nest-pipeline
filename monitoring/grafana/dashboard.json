{
  "dashboard": {
    "title": "NestJS Pipeline - Stress Test",
    "panels": [
      {
        "id": 1,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[1m]) * 100"
          }
        ]
      },
      {
        "id": 2,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024"
          }
        ]
      },
      {
        "id": 3,
        "title": "RabbitMQ Queue Size",
        "type": "graph",
        "targets": [
          {
            "expr": "rabbitmq_queue_messages"
          }
        ]
      }
    ]
  }
}
